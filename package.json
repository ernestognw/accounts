{
  "name": "@openzeppelin/accounts",
  "version": "0.0.1",
  "description": "A Javascript library to use OpenZeppelin Smart Accounts",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run clean",
    "prepare": "npm run compile",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "test": "ava",
    "test:watch": "ava -w",
    "test:coverage": "c8 npm run test --"
  },
  "workspaces": {
    "packages": [
      "packages/core/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenZeppelin/accounts.git"
  },
  "dependencies": {
    "@noble/curves": "^1.9.0",
    "viem": "^2.28.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.25.1",
    "@fast-check/ava": "^2.0.2",
    "ava": "^6.2.0",
    "c8": "^10.1.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "fast-check": "^4.1.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "evm",
    "smart-accounts"
  ],
  "author": "Ernesto Garc√≠a <ernestognw@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenZeppelin/accounts/issues"
  },
  "homepage": "https://github.com/OpenZeppelin/accounts#readme"
}
